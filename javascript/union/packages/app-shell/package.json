{
  "name": "@h4h/app-shell",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattpocock/tt-package-demo.git"
  },
  "files": [
    "lib"
  ],
  "exports": {
    "./app-shell.css": "./lib/index.css",
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@types/react": "19.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6",
    "postcss": "8.5.3"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "scripts": {
    "build": "tsup",
    "typelint": "tsc",
    "ci": "pnpm run build && pnpm run check-format && pnpm run check-exports && pnpm run test",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev2": "vitest",
    "dev": "tsup --watch",
    "css": "pnpm dlx @tailwindcss/cli -i ./src/app-shell.css -o ./lib/app-shell.css"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.7",
    "@tailwindcss/postcss": "^4.0.8",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^4.0.7",
    "@headlessui/react": "2.2.0",
    "framer-motion": "12.4.7",
    "@heroicons/react": "2.2.0"
  }
}